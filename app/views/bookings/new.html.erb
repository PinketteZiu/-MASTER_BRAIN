<h1>New Booking</h1>

<%= simple_form_for([@booking.brain, @booking]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start_date, required: true %>
    <%= f.input :end_date, required: true %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Create Booking" %>
  </div>
<% end %>
